                            <tr>
                              <td width="100%" align="center" style="font-size:26px; color:#2a2d32; font-family:'Lato',Arial, sans-serif; font-weight:700;">Nombre: {{customer.firstName}} {{customer.lastName}} </td>
                            </tr>
                          </table>
                        </td>
                      </tr>
                    </table>
                    <table align="center" width="100%" border="0" cellspacing="0" cellpadding="0" object="drag-module-small">
