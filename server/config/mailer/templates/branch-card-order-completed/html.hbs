                        <td height="20" class="em_h30">
                          <img src="images/spacer.gif" width="1" alt="" height="1" border="0" style="display:block;" />
                        </td>
                      </tr>
                    </table>
                    <!-- fin tabala-->



                    <table width="300" border="0" cellpadding="0" cellspacing="0" align="center" class="full" object="drag-module-small">
                      <tr>
                        <td valign="top" class="em_pad2">
                          <table align="center" width="500" border="0" cellspacing="0" cellpadding="0" class="full">
                            <tr>
                              <td valign="middle" align="left" width="63%" style="font-size:13px; color:#aaaaaa; font-family:'Lato',Arial, sans-serif; font-weight:400;">
                                Paquete/producto
                              </td>
                              <td width="25%" align="left" style="font-size:13px; color:#aaaaaa; font-family:'Lato',Arial, sans-serif; font-weight:400;">
                                Precio
                              </td>
                              {{#if totalPending}}
                              <td width="25%" align="left" style="font-size:13px; color:#aaaaaa; font-family:'Lato',Arial, sans-serif; font-weight:400;">
                                Pago
                              </td>
                              <td width="25%" align="left" style="font-size:13px; color:#aaaaaa; font-family:'Lato',Arial, sans-serif; font-weight:400;">
                                Pendiente
                              </td>
                              {{/if}}
                            </tr>
                            <tr>
                              <td height="15" bgcolor="#fafafa">
                                <img src="http://www.wmu.mx/images/spacer.gif" width="1" alt="" height="1" border="0" style="display:block;" />
                              </td>
                            </tr>
                            {{#each items as | item |}}
                            <tr>
                              <td valign="middle" align="left" width="63%" style="font-size:13px; color:#aaaaaa; font-family:'Lato',Arial, sans-serif; font-weight:400;">
                                <span style="font-size:15px; color:#ff7101; font-family:'Lato',Arial, sans-serif; font-weight:700;">{{item.parent.parent.name}} ({{this.itemDetails.name}})</span>
                              </td>
                              <td width="25%" align="left" style="font-size:13px; color:#725858; font-family:'Lato',Arial, sans-serif; font-weight:400;">
                                <span style="font-size:15px; color:#aaaaaa; font-family:'Lato',Arial, sans-serif; font-weight:700;"> ${{item.price}}</span>
                              </td>
                              {{#if ../totalPending}}
                              <td width="25%" align="left" style="font-size:13px; color:#aaaaaa; font-family:'Lato',Arial, sans-serif; font-weight:400;">
                                <span style="font-size:15px; color:#aaaaaa; font-family:'Lato',Arial, sans-serif; font-weight:700;"> ${{item.firstPaymentAmount}}</span>
                              </td>
                              <td width="25%" align="left" style="font-size:13px; color:#aaaaaa; font-family:'Lato',Arial, sans-serif; font-weight:400;">
                                <span style="font-size:15px; color:#aaaaaa; font-family:'Lato',Arial, sans-serif; font-weight:700;"> ${{item.pending}}</span>
                              </td>
                              {{/if}}
                            </tr>
                            <tr>
                              <td height="15" bgcolor="#fafafa">
                                <img src="http://www.wmu.mx/images/spacer.gif" width="1" alt="" height="1" border="0" style="display:block;" />
                              </td>
                            </tr>
                            {{/each}}
                            <tr>
                              <td valign="middle" align="left" width="63%" style="font-size:13px; color:#aaaaaa; font-family:'Lato',Arial, sans-serif; font-weight:400;">
                                Total
                              </td>
                              <td width="25%" align="left" style="font-size:13px; color:#aaaaaa; font-family:'Lato',Arial, sans-serif; font-weight:400;">
                                ${{total}}
                              </td>
                              {{#if totalPending}}
                              <td width="25%" align="left" style="font-size:13px; color:#aaaaaa; font-family:'Lato',Arial, sans-serif; font-weight:400;">
                                ${{totalFirstPayment}}
                              </td>
                              <td width="25%" align="left" style="font-size:13px; color:#aaaaaa; font-family:'Lato',Arial, sans-serif; font-weight:400;">
                                ${{totalPending}}
                              </td>
                              {{/if}}
                            </tr>
                          </table>
                        </td>
                      </tr>
                    </table>
                    <table width="100%" border="0" cellpadding="0" cellspacing="0" align="center" style="text-align: center; border-collapse:collapse; mso-table-lspace:0pt; mso-table-rspace:0pt;"
                      class="fullCenter">
                      <tr>
                        <td width="100%" height="30" class="em_h30">
                          <img src="images/spacer.gif" width="1" alt="" height="1" border="0" style="display:block;" />
                        </td>
                      </tr>
                    </table>

