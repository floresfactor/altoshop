                        <td height="10">
                          <img src="images/spacer.gif" width="1" alt="" height="1" border="0" style="display:block;" />
                        </td>
                      </tr>
                    </table>

                    <table width="250" border="0" cellpadding="0" cellspacing="0" align="center" class="full" object="drag-module-small">
                      <tr>
                        <td align="center" width="250" valign="middle" class="em_pad">

                          <table width="100%" border="0" cellpadding="0" cellspacing="0" align="center" style="text-align: center; border-collapse:collapse; mso-table-lspace:0pt; mso-table-rspace:0pt;"
                            class="fullCenter">
                            <tr>
                              <td width="100%" align="center" style="font-size:14px; color:#2a2d32; font-family:'Lato',Arial, sans-serif; font-weight:400; line-height:22px;">
                                <strong>id</strong>: {{customer._id}} &nbsp; &nbsp;
                                <span style="color:#e9ebf4;"> | </span> &nbsp; &nbsp;
                                <span style="color:#2a2d32;">
                                  <strong>correo</strong>: {{customer.email}}</span>
                              </td>
                            </tr>
                          </table>
                        </td>
                      </tr>
                    </table>
                    <!-- principio tabla-->
                    <table align="center" width="100%" border="0" cellspacing="0" cellpadding="0" object="drag-module-small">
